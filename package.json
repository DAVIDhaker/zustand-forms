{
  "name": "zustand-forms",
  "version": "0.2.3",
  "description": "Form validator for Zustand",
  "scripts": {
    "build": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "github.com/davidhaker/zustand-forms"
  },
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/validators": {
      "default": {
        "types": "./dist/validators.d.ts",
        "default": "./dist/validators.js"
      }
    }
  },
  "type": "module",
  "keywords": [
    "zustand",
    "forms",
    "validators",
    "validate"
  ],
  "author": "Mr. DAVIDhaker",
  "license": "MIT",
  "dependencies": {
    "zustand": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^4.5.2"
  }
}
